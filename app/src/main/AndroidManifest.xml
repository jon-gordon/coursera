<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jongor_software.android.learning.coursera">

    <application android:allowBackup="true"
        android:label="@string/app_name"
        android:icon="@mipmap/ic_launcher"
        android:theme="@style/AppTheme">

        <activity android:name=".PMAAHS1.Week2.ActivityOne"></activity>
        <activity android:name=".PMAAHS1.Week2.ActivityTwo"></activity>

        <activity android:name=".PMAAHS1.Week3.Intents.MyBrowserActivity">
            <!-- Add necessary intent filter information so that this Activity will accept
                 Intents with the action "android.intent.action.VIEW" and with a "http" schemed
                 URL -->
            <intent-filter>
                <action android:name="android.intent.action.VIEW"></action>
                <category android:name="android.intent.category.DEFAULT"></category>
                <category android:name="android.intent.category.BROWSABLE"></category>
                <data android:scheme="http"></data>
            </intent-filter>
        </activity>
        <activity android:name=".PMAAHS1.Week3.Intents.ActivityLoaderActivity"></activity>
        <activity android:name=".PMAAHS1.Week3.Intents.ExplicitlyLoadedActivity"></activity>

    </application>

</manifest>
